{
    "name": "bilibili",
    "version": "1.0.0",
    "main": "src/main.js",
    "scripts": {
        "start": "electron .",
        "package": "electron-packager . --win --electron-version=1.7.10 --out dist/ --icon=img/bili.ico",
        "debian": "electron-installer-debian --src dist/bilibili-linux-x64/ --dest dist/installers/ --arch amd64",
        "windows": "electron-installer-windows --src dist/bilibili-win32-x64/ --dest dist/installers/ --arch amd64"
    },
    "devDependencies": {
        "electron-installer-windows": ""
    }
}
